{
  "cli": {
    "version": ">= 2.5.1"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true,
        "enterpriseProvisioning": "universal"
      },
      "channel": "development"
    },
    "preview-ios": {
      "distribution": "internal",
      "developmentClient": true,
      "channel": "preview-ios"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "channel": "preview"
    },
    "preview2": {
      "android": {
        "gradleCommand": ":app:assembleDebug"
      },
      "channel": "preview2"
    },
    "production": {
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
